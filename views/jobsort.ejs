<%- include('partials/header') %>
    <title>JobSort</title>
</head>
<body>
    <a href="/home" style="text-decoration: none; margin-right: 2%;color:brown;font-weight:bolder;font-size:x-large;display:flex;justify-content: end;">Home</a>
    <h1 style="font-weight:900;text-align:center;margin:2rem;">JOB sorted</h1>
    <table class="table table-striped table-dark">
        <thead class="thead-dark">
          <tr>
            <td>Name</td>
            <td>Standard</td>
            <td>Organization</td>
            <td>Salary</td>
            <td>Skills</td>
        </tr>
        <% for(let i of temp){ %>
            <tr>
                <td><%= i.name %></td>
                <td><%= i.standard %></td>
                <td><%= i.organization %></td>
                <td><%= i.salary %></td>
                <td><%= i.skills %></td>
            </tr>
        <% } %>
    </table>


</body>
</html>