<%- include('partials/header') %>
    <title>Your sorted choices</title>
</head>
<body>
    <a href="/home" style="text-decoration: none; margin-right: 2%;color:brown;font-weight:bolder;font-size:x-large;display:flex;justify-content: end;">Home</a>
    <h1 style="font-weight:900;text-align:center;margin:5rem;">
        Options Based on your filters
    </h1>
    <table class="table table-striped table-dark">
        <thead class="thead-dark">
          <tr>
            <th scope="col-2">Family Income</th>
            <th scope="col-6">Reward</th>
            <th scope="col-2">Percentage</th>
        </tr>
        <% for(let i of temp){ %>
            <tr>
                <td><%= i.familyincome %></td>
                <td><%= i.reward %></td>
                <td><%= i.percentage %></td>
            </tr>
        <% } %>
    </table>
</body>
</html>
    